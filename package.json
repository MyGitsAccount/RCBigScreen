{
  "name": "rongchang-big-screen",
  "private": false,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.jsx,.tsx,.vue,.d.ts --ignore-path .gitignore --fix src && prettier --write --ignore-unknown",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": [
      "eslint --fix --ignore-path",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bitmap3d": "^0.0.64",
    "echarts": "^5.3.2",
    "echarts-gl": "^2.0.9",
    "mars3d-echarts": "^3.3.10",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15",
    "vue3-seamless-scroll": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.2",
    "naive-ui": "^2.29.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-mars3d": "^1.1.3",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vue-tsc": "^0.34.7"
  }
}
